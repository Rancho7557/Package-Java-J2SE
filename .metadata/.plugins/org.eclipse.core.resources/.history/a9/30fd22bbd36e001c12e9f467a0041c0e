package javacode;

public class Oops_Constructor {
	
	int a; String name;
	
	Oops_Constructor()
	   {
		a = 0; name = null;
	   }
		void show()
		{
			System.out.println(a + "    " + name);
		}
		class B 
		{
			
		}

	public static void main(String[] args) {
		 
		Oops_Constructor ref = new Oops_Constructor();
		ref.show();

	}

}
